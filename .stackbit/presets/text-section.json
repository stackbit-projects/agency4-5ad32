{
    "model": "TextSection",
    "presets": [{
            "label": "Full width, center",
            "thumbnail": "images/placeholder.png",
            "data": {
                "colors": "colors-a"
            },
            "styles": {
                "self": {
                    "margin": ["mt-0", "mb-0"],
                    "flexDirection": "row-reverse"
                },
                "quote": {
                    "textAlign": "right"
                },
                "name": {
                    "fontWeight": 400,
                    "fontStyle": "normal",
